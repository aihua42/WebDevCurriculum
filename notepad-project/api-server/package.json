{
  "name": "api-server",
  "version": "1.0.0",
  "description": "api server of notepad project",
  "main": "api.mjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "api": "nodemon originals/api.mjs",
    "start": "이거 안됨: nodemon --exec babel-node src/api.mts", 
    "build-api": "babel src --out-dir dist && tsc --noEmit",
    "build-helpers": "babel helpers-src --out-dir helpers && tsc --noEmit",
    "build-models": "babel models-src --out-dir models && tsc --noEmit",
    "build-mid": "babel middlewares-src --out-dir middlewares && tsc --noEmit",
    "build-contr": "babel controllers-src --out-dir controllers && tsc --noEmit",
    "build-routes": "babel routes-src --out-dir routes && tsc --noEmit",
    "lint-api": "eslint --no-cache src/**/*.mts --fix",
    "lint-types": "eslint --no-cache types.ts --fix",
    "lint-helpers": "eslint --no-cache helpers-src/**/*.mts --fix",
    "lint-models": "eslint --no-cache models-src/**/*.ts --fix",
    "lint-mid": "eslint --no-cache middlewares-src/**/*.mts --fix",
    "lint-contr": "eslint --no-cache controllers-src/**/*.mts --fix",
    "lint-routes": "eslint --no-cache routes-src/**/*.mts --fix"
  },
  "author": "AihuaJin",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "connect-session-sequelize": "^7.1.7",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.35.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/node": "^7.23.9",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.23.8",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.6",
    "@types/sequelize": "^4.28.20",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "babel-plugin-module-extension": "^0.1.3",
    "babel-watch": "^7.8.1",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "standard": "^17.1.0",
    "tsx": "^4.7.0"
  }
}
